{
  "name": "icomfort",
  "version": "1.4.6",
  "description": "A JavaScript client for interacting with the Lennox iComfort website",
  "keywords": [
    "node",
    "javascript",
    "lennox",
    "icomfort",
    "thermostat",
    "automation"
  ],
  "main": "./src/index.js",
  "files": [
    "/src/**/*.js",
    "!/src/**/*.spec.js"
  ],
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "lint": "eslint --fix ./src/ ./tests/",
    "coverage": "npx reload -b -d coverage/",
    "test": "mocha tests/*",
    "test:cov": "nyc npm t"
  },
  "author": "Daniel McQuiston <daniel.pro@dehugo.net>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "eslint": "^7.26.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0"
  },
  "engines": {
    "node": ">=10"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "check-coverage": true,
    "statements": 94,
    "branches": 95,
    "functions": 88,
    "lines": 93
  },
  "repository": "github:deHugo/icomfort-js"
}
